# Add custom bin directory
if [ -d "$HOME/devel/dotfiles/bin" ] ; then
    PATH="$HOME/devel/dotfiles/bin:$PATH"
fi

# Environment variables
# Gems location
export RUBYLIB=$HOME/Applications/rubygems/lib
export GEM_HOME=$HOME/Applications/rubygems/gems
export PATH=$HOME/Applications/rubygems/bin:$GEM_HOME/bin:$PATH
